		"doctrine/dbal": "3.0.0",
		"egulias/email-validator": "2.1.25",
		"giggsey/libphonenumber-for-php": "^8.12",
		"guzzlehttp/guzzle": "^7.2",
		"icewind/searchdav": "^2.0.0",
		"icewind/streams": "v0.7.2",
		"league/flysystem": "^1.0",
		"microsoft/azure-storage-blob": "^1.5",
